<?xml version='1.0' encoding='utf-8'?>


<widget id="com.phonegap.telnummer"

version="1.0.0" 
xmlns="http://www.w3.org/ns/widgets" 

versionCode = "1"

xmlns:gap="http://phonegap.com/ns/1.0">
  

<name>TelNummer</name>
    

<description>TelNummer</description>
  

<author  email="veter_peremen_i@mail.ru" href="http://phonegap.com">
        Polina Wilke    </author>
 
 <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />

	<content src="index.html" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
<allow-intent href="geo:*" />
<allow-intent href="sms:*" />
<gap:plugin name="cordova-plugin-sms" source="npm" />
<plugin name="cordova-plugin-whitelist" spec="1.1.0" />
<gap:plugin name="cordova-plugin-sim2" source="npm" />




</widget>
